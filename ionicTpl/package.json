{
    "name": "Convertigo-template-app",
    "author": "Convertigo & Ionic Framework",
    "homepage": "http://www.convertigo.com/",
    "private": true,
    "scripts": {
        "test": "webdriver-manager update && webdriver-manager start --detach && protractor ./e2e/protractor.conf.js",
        "ngsw-config": "node_modules/.bin/ngsw-config ../DisplayObjects/mobile src/ngsw-config.json /convertigo/projects/{{c8o_project}}/DisplayObjects/mobile/",
        "clean": "ionic-app-scripts clean",
        "build": "npm run wv && ionic-app-scripts build --enableLint false --generateSourceMap false",
        "ionic:build": "npm run wv && ionic-app-scripts build --enableLint false",
        "ionic:build:prod": "npm run wv && node --max_old_space_size=8192 ./node_modules/@ionic/app-scripts/bin/ionic-app-scripts.js  build --prod --enableLint false && npm run ngsw-config",
        "ionic:serve:eval": "npm run wv && ionic-app-scripts serve --enableLint false --sourceMapType eval",
        "ionic:serve:nosourcemap": "npm run wv && ionic-app-scripts serve --enableLint false --generateSourceMap false",
        "ionic:serve": "npm run wv && ionic-app-scripts serve --enablelint false && npm run ngsw-config",
        "wv": "node ./version.js"
    },
    "dependencies": {
        "@angular/animations": "5.2.11",
        "@angular/common": "5.2.11",
        "@angular/compiler": "5.2.11",
        "@angular/compiler-cli": "5.2.11",
        "@angular/core": "5.2.11",
        "@angular/forms": "5.2.11",
        "@angular/http": "5.2.11",
        "@angular/platform-browser": "5.2.11",
        "@angular/platform-browser-dynamic": "5.2.11",
        "@angular/service-worker": "5.2.11",
        "@ionic-native/core": "~4.20.0",
        "@ionic-native/splash-screen": "~4.20.0",
        "@ionic-native/status-bar": "~4.20.0",
        "@ionic/storage": "2.2.0",
        "ionic-angular": "3.9.9",
        "ionicons": "3.0.0",
        "rxjs": "6.3.3",
        "sw-toolbox": "3.6.0",
        "zone.js": "0.11.3",
        "@ngx-translate/core": "9.0.2",
        "@ngx-translate/http-loader": "2.0.1",
        "animate.css": "3.7.0",
        "c8osdkangular": "3.0.16-beta37",
        "c8ocaf": "1.1.15",
        "@types/node": "12.12.6"
    },
    "devDependencies": {
        "@ionic/app-scripts": "3.2.4",
        "typescript": "~2.6.2",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "codelyzer": "^6.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "html-dnd": "1.2.1",
        "selenium-webdriver": "4.0.0-alpha.7",
        "node-jq": "1.5.0",
        "js-yaml": "4.1.0"
    },
    "description": "MyApp: An Ionic project",
    "cordovaPlugins": [
        "cordova-plugin-device",
        "cordova-plugin-console",
        "cordova-plugin-whitelist",
        "cordova-plugin-splashscreen",
        "cordova-plugin-statusbar",
        "ionic-plugin-keyboard"
    ],
    "config": {
        "ionic_copy": "./config/copy.config.js",
        "ionic_watch": "./config/watch.config.js",
        "ionic_www_dir": "../DisplayObjects/mobile",
        "ionic_sass": "./config/sass.config.js"
    },
    "cordovaPlatforms": []
}